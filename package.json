{
  "name": "setup-backend-express-typescript",
  "version": "1.0.0",
  "description": "Un template basico para el inicio de un backend con node.js",
  "main": "./build/index.js",
  "scripts": {
    "start": "DEBUG=Server:* node ./build/index.js",
    "dev": "DEBUG=Server:* nodemon",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --detectOpenHandles --coverage",
    "build": "tsc",
    "prettier:write": "prettier ./src --write",
    "prettier:check": "prettier ./src --check",
    "eslint:fix": "eslint ./src --ext .ts --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*": [
      "npm run prettier:write",
      "npm run eslint:fix"
    ],
    "*": [
      "jest --detectOpenHandles --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaestroShifu/setup-backend-express-typescript.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "expressjs",
    "express"
  ],
  "author": "MaestroShifu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaestroShifu/setup-backend-express-typescript/issues"
  },
  "homepage": "https://github.com/MaestroShifu/setup-backend-express-typescript#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.5",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0"
  }
}
